{
    "env": {
      "browser": false,
      "es2021": true,
      "node": true},
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "no-var": "error",
      "prefer-const": "warn",
      "eqeqeq": "error",
      "curly": "error",
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "semi": true,
          "tabWidth": 2,
          "trailingComma": "es5"
        }
      ]
    }
  }